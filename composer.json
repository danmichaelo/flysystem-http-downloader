{
    "name": "indigophp/flysystem-http-downloader",
    "description": "Download any HTTP content to a file",
    "license": "MIT",
    "keywords": ["flysystem", "http"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "league/flysystem": "~1.0",
        "php-http/httplug": "~1.1",
        "php-http/client-implementation": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Flysystem\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
